<style>

    @@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@200;300;400;500&display=swap');


    .fecha {
        padding-left: 120px;
    }

    * {
        font-size: 12px;
        font-family: 'Montserrat', sans-serif;
        font-weight: 400;
    }

    .titulo {
        background-color: #c2c2c2;
        font-size: 11px;
        font-weight: bold;
    }

    .titulos {
        background-color: #c2c2c2;
        font-size: 11px;
        font-weight: bold;
        margin-left: 20px;
        margin-right: 20px;
    }

    .egt {
        font-size: 10px;
        width: 100%;
    }

    .egt2 {
        font-size: 10px;
    }

    .espacio {
        padding-right: 15px;
    }

    .borde {
        border: 1px solid black;
        padding: 5px;
        margin: 0px;
    }

    .uno {
        padding-right: 60px;
        padding-left: 60px;
    }

    .dos {
        padding-right: 70px;
        padding-left: 70px;
    }

    .tres {
        padding-right: 110px;
        padding-left: 110px;
    }

    .parrafo {
        margin: 0px;
        padding: 0px;
        font-size: 13px;
    }

    .parrafo2 {
        text-align: justify;
        margin-top: 5px;
        margin-bottom: 5px;
        font-size: 13px;
        padding: 10px 0;
    }

    .base {
        background-color: #c2c2c2;
        font-weight: bold;
        margin-top: 10px;
        margin-bottom: 20px;
        font-size: 11px;
        width: 100%;
    }


    .base2 {
        background-color: #c2c2c2;
        font-weight: bold;
        margin-top: 15px;
        margin-bottom: 10px;
        font-size: 11px;
    }

    #encabezado {
        text-align: center;
        margin-bottom: 5px;
        font-size: 14px !important;
    }

    .azul {
        background-color: #9ac5ed;
        fill: #9ac5ed;
    }


    .verde {
        background-color: #6bed4e;
        fill: #6bed4e;
    }

    .amarillo {
        background-color: #fff86b;
        fill: #fff86b;
    }

    .naranja {
        background-color: #e88a2c;
        fill: #e88a2c;
    }

    .rojo {
        background-color: #bd5151;
        fill: #bd5151;
    }


    .referencia {
        width: 98%;
        height: 100%;
        border: 2px black;
    }

    .info-general {
        width: 100%;
    }

    .white {
        border: white;
    }

    .explicacion {
        margin-bottom: 10px;
        font-weight: bold;
        padding-right: 20px;
    }

    .aplicacion {
        margin-top: 50px;
        text-align: center;
        font-weight: bold;
    }

    .desc {
        font-weight: bold;
        padding: 10px;
        width: 250px;
    }

    .val {
        padding-left: 10px;
    }

    .cabezera-table {
        padding: 40px 0;
        font-weight: bold;
    }

    .tamano {
        padding: 10px 0;
    }

    .tamano2 {
        font-size: 12px;
    }


</style>



<div id="encabezado">
    <b>INFORME EJECUTIVO DE RESULTADOS</b>
</div>
<div id="encabezado">
    <b> @Request.Params["norma"].ToString() </b>
</div>
<div id="encabezado">
    <b>  @Request.Params["titulo"].ToString() </b>
</div>
<div class="base borrado">DATOS GENERALES:</div>
<div class="margen">

    <table class="egt white">

        <tr>
            <td width="270"><b>Empresa</b></td>
            <td> @Request.Params["empresa"].ToString()</td>
            <td class="fecha"></td>
        </tr>
        @if (Request.Params["ternario"].ToString().Equals("true"))
        {


            <tr>
                <td width="270" class="espacio"><b>Centro de Trabajo:</b></td>
                <td> @Request.Params["centro"].ToString()</td>
                <td></td>
            </tr>


            <tr>
                <td width="270" class="espacio"><b>Domicilio del Centro de Trabajo:</b></td>
                <td> @Request.Params["domicilio"].ToString()</td>
                <td></td>
            </tr>
        }
        else
        {

            <tr>
                <td width="270" class="espacio"><b>Domicilio de la Empresa:</b></td>
                <td> @Request.Params["domicilio"].ToString()</td>
                <td></td>
            </tr>
        }

        <tr>
            <td width="270" class="espacio"><b>Actividad de la Empresa:</b></td>
            <td> @Request.Params["actividad"].ToString()</td>
            <td></td>
        </tr>


        <tr>
            <td width="270" class="espacio"><b>Fecha:</b></td>
            <td> @Request.Params["fecha"].ToString()</td>
            <td></td>
        </tr>


    </table>
</div>


