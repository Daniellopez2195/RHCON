@model  rhcon.Models.ViewModel.AceptaPoliticasViewModel
@{
    ViewBag.Title = "Aceptación de Políticas";
    Layout = "~/Views/Shared/_LayoutCrud.cshtml";
}

<br />

@if (ViewBag.Message != null)
{
    <div class="alert-danger" role="alert">
        @ViewBag.Message
    </div>
}



<br />
@using (Html.BeginForm("Save", "Politica", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken();
<div class="row">

    <div class="col-12 col-md-6 d-flex justify-content-center align-items-center flex-column">
        <h1>¡TE DAMOS LA BIENVENIDA!</h1>
        <h4>@Html.ViewBag.empleado</h4>

        <p>Antes de accesar a la pantalla principal, por favor, realiza los pasos siguientes:</p>
        <ol class="lista">
            <li>Selecciona las políticas.</li>
            <li>Descárgalas y guárdalas en tu dispositivo.</li>
            <li>Realiza una lectura de compresión.</li>
            <li>Sube las políticas una vez que las hayas leído y comprendido.</li>
            <li>Acepta las políticas.</li>
        </ol>
    </div>
    <div class="col-12 col-md-6">
        <div class="form-group">
            <div>
                <a href="~/Empleado/VideoNom035" target="_blank">Ver video de la NOM-035-STPS-2018  </a>
            </div>
            <div class="descargaPolitica">
                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="2084" height="2084" viewBox="0 0 2084 2084">
                    <image id="Capa_1" data-name="Capa 1" x="206" y="339" width="1672" height="1405" xlink:href="data:img/png;base64," />
                </svg>
                @Html.ActionLink("Política por un Entorno Organizacional Favorable y la Prevención de Riesgos Psicosociales", "VistaPdf/4", "Politica", new { @class = "pdfCargado", target = "_blank" })
            </div>

            @Html.TextBoxFor(d => d.nom035, new { @class = "form-control", type = "file", accept = ".pdf" })
            @Html.ValidationMessageFor(d => d.nom035, "", new { @class = "text-danger" })
        </div>
        <div class="form-group">
            <br />
            <div>
                <a href="~/Empleado/VideoNom025" target="_blank">Ver video de la NMX-025-SCFI-2015</a>
            </div>
            <div class="descargaPolitica">
                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="2084" height="2085" viewBox="0 0 2084 2085">
                    <image id="Capa_1" data-name="Capa 1" x="297" y="96" width="1490" height="1866" xlink:href="data:img/png;base64," />
                </svg>
                @Html.ActionLink("Política de Igualdad y No Discriminación", "VistaPdf/5", "Politica", new { @class = "pdfCargado", target = "_blank" })
            </div>


            @Html.TextBoxFor(d => d.nom025, new { @class = "form-control", type = "file", accept = ".pdf" })
            @Html.ValidationMessageFor(d => d.nom025, "", new { @class = "text-danger" })
        </div>
        <div class="form-group d-flex justify-content-center pt-4">
            <div>
                <label>He leído, comprendido y acepto las políticas </label>
                @Html.CheckBoxFor(d => d.AceptoPolitica)
                @Html.ValidationMessageFor(d => d.AceptoPolitica, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="d-flex justify-content-center pt-4">
            <input type="submit" class="btn btn-success" value="Aceptar" />
        </div>
        <br />
        <a href="~/Home/Logout" style="float:right;" class="btn btn-primary">Salir</a>
    </div>
 
</div>

}
