        border-radius: 4px;
        color: #666665;
        padding: 10px 0;
    }

    option {
        text-align: left;
    }

    input {
        width: 100%;
        border: 1.5px solid #878787;
        border-radius: 4px;
        color: #666665;
        padding: 8px 0;
    }

    .boton-enviar{
        width:210px;
    }

    .boton-personalizado {
        text-decoration: none;
        font-size: 15px;
        color: #333333;
        padding-top: 15px;
        padding-bottom: 15px;
        padding-left: 50px;
        padding-right: 50px;
        background-color: #ffffff;
        border-color: #878787;
        border-width: 1px;
        border-style: solid;
        border-radius: 4px;
