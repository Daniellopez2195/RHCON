        -moz-border-radius: 50%;
        -webkit-border-radius: 50%;
        border-radius: 50%;
        background: #ff7b08;
    }

    .circulo-nivel {
        width: 25px;
        height: 25px;
        -moz-border-radius: 50%;
        -webkit-border-radius: 50%;
        border-radius: 50%;
        background: #ff7b08;
    }

    select {
        display: block;
        text-align: center;
        text-align-last: center;
